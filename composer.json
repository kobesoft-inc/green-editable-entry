{
    "name": "kobesoft/green-editable-entry",
    "description": "Editable entry component for Filament with inline editing capabilities",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Green\\EditableEntry\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^12.0",
        "filament/filament": "^4.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Green\\EditableEntry\\EditableEntryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
